{
  "name": "nyx-player",
  "type": "module",
  "version": "0.0.1",
  "exports": {
    ".": {
      "import": "./src/entries/init.ts",
      "browser": "./dist/player.js",
      "require": "./dist/player.cjs"
    },
    "./components": {
      "import": "./src/entries/components.ts",
      "browser": "./dist/components.js",
      "require": "./dist/components.cjs"
    },
    "./style": "./dist/nyx-player.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:preview": "vue-tsc -b && vite build --config vite.config.preview.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@iconify-json/ri": "^1.2.5",
    "es-toolkit": "^1.31.0",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "stylus": "^0.64.0",
    "unocss": "^0.65.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@unocss/eslint-plugin": "^0.65.3",
    "@unocss/preset-icons": "^0.65.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
    "rollup-plugin-visualizer": "^5.13.1",
    "typescript": "~5.7.2",
    "vite": "^6.0.6",
    "vue-tsc": "^2.2.0"
  }
}
